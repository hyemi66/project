<?xml version="1.0" encoding="UTF-8"?>

<!-- 이 파일의 역할은 데이터 저장빈 클래스의 객체 별칭이름을 관리한다 -->
<!DOCTYPE configuration
PUBLIC "-//mybatis.org//DTD config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<typeAliases>
		
		<!-- BbsVO 빈클래스 객체 별칭이름을 bbs로 지정 -->
		<typeAlias type="net.daum.vo.BbsVO" alias="bbs" />
		
		<!-- PageVO 빈클래스 객체 별칭이름을 page로 지정 -->
		<typeAlias type="net.daum.vo.PageVO" alias="page" />
		
		<!-- AdminVO 빈클래스 객체 별칭이름을 admin으로 지정 -->
		<typeAlias type="net.daum.vo.AdminVO" alias="admin" />
		
		<typeAlias type="net.daum.vo.MemberVO" alias="member" />
		
		<typeAlias type="net.daum.vo.ZipCodeVO" alias="zipcode" />
		
	</typeAliases>
</configuration>